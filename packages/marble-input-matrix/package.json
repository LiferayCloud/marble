{
  "name": "marble-input-matrix",
  "version": "2.1.15",
  "description": "Marble's input matrix component",
  "license": "BSD-3-Clause",
  "repository": "https://github.com/wedeploy/marble/tree/master/packages/marble-input-matrix",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
