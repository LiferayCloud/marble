{
  "name": "marble-input-matrix",
  "version": "2.1.13",
  "description": "Metal.js component that automatically adds new fields to guarantee that there will always be an empty field at the end",
  "license": "BSD",
  "repository": "wedeploy/marble",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "main": "lib/InputMatrix.js",
  "jsnext:main": "src/InputMatrix.js",
  "files": [
    "lib",
    "src",
    "test"
  ],
  "scripts": {
    "build": "npm run soy && webpack",
    "compile": "babel --presets metal -d lib/ src/",
    "prepublish": "npm run soy && npm run compile",
    "soy": "metalsoy --soyDeps node_modules/marble*/src/**/*.soy",
    "test": "karma start karma.conf.js"
  },
  "keywords": [
    "metal"
  ],
  "dependencies": {
    "marble-input": "^1.0.15",
    "metal-soy": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-node-env-inline": "^0.2.0",
    "babel-preset-metal": "^4.0.0",
    "chai": "^4.1.2",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.6",
    "metal-tools-soy": "^4.2.6",
    "mocha": "^4.0.1",
    "webpack": "^3.5.5"
  }
}
