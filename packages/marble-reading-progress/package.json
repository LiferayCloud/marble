  "keywords": [
    "metal"
  ],
  "dependencies": {
    "metal-scrollspy": "^2.0.0",
    "metal-soy": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-node-env-inline": "^0.2.0",
    "babel-preset-metal": "^4.0.0",
    "bootstrap": "^3.3.6",
    "gulp": "^3.8.11",
    "gulp-metal": "^1.0.0",
    "marble": "github:wedeploy/marble#master",
    "metal-tools-soy": "^4.2.6",
    "webpack": "^3.5.5"
  }
}
