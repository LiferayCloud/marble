{
  "name": "marble-reading-progress",
  "version": "2.0.0",
  "description": "A reading progress indicator",
  "license": "BSD",
  "repository": "wedeploy/marble",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "jsnext:main": "src/ReadingProgress.js",
  "main": "lib/ReadingProgress.js",
  "files": [
    "lib",
    "src",
    "test"
  ],
  "scripts": {
    "build": "npm run soy && webpack",
