{
  "name": "marble-slider",
  "version": "2.1.4",
  "description": "Crystal's slider component",
  "license": "BSD",
  "repository": "wedeploy/marble",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "jsnext:main": "src/Slider.js",
  "main": "lib/Slider.js",
  "files": [
    "lib",
    "src",
    "test"
  ],
  "scripts": {
    "build": "npm run soy && webpack",
    "compile": "babel --presets metal -d lib/ src/",
    "prepublish": "npm run soy && npm run compile",
    "soy": "metalsoy --soyDeps node_modules/metal*/src/**/*.soy",
    "test": "gulp test"
  },
  "keywords": [
    "metal"
  ],
  "dependencies": {
    "metal": "^2.0.0",
    "metal-component": "^2.0.0",
    "metal-dom": "^2.0.0",
    "metal-drag-drop": "^2.0.0",
    "metal-position": "^2.0.0",
    "metal-soy": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-node-env-inline": "^0.2.0",
    "babel-preset-metal": "^4.0.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "gulp": "^3.8.11",
    "gulp-metal": "^1.0.0",
    "karma-scss-preprocessor": "^1.0.1",
    "metal-karma-config": "^2.0.0",
    "metal-tools-soy": "^4.2.6",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5"
  }
}
