{
  "name": "marble-tooltip",
  "version": "1.0.2",
  "description": "Marble's metal tooltip component",
  "license": "BSD",
  "repository": "wedeploy/marble",
  "author": {
    "name": "Ygor Costa",
    "email": "ygor.costa@liferay.com"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "jsnext:main": "src/Tooltip.js",
  "main": "lib/Tooltip.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "npm run soy && webpack",
    "compile": "babel --presets metal -d lib/ src/",
    "prepublish": "npm run soy && npm run compile",
    "soy": "metalsoy --soyDeps node_modules/metal*/src/**/*.soy",
    "test": "karma start karma.conf.js"
  },
  "keywords": [
    "marble",
    "metal",
    "tooltip"
  ],
  "dependencies": {
    "metal": "^2.0.0",
    "metal-component": "^2.0.0",
    "metal-dom": "^2.0.0",
    "metal-events": "^2.0.0",
    "metal-position": "^2.0.0",
    "metal-soy": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-node-env-inline": "^0.2.0",
    "babel-preset-metal": "^4.0.0",
    "chai": "^4.1.2",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.6",
    "metal-tools-soy": "^4.2.6",
    "mocha": "^4.0.1",
    "webpack": "^3.5.5"
  }
}
